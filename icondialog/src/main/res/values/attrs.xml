<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <attr name="icdStyle" format="reference"/>
    
    <declare-styleable name="IconDialog">
        <attr name="icdBackgroundColor" format="color|reference"/>
        <attr name="icdMaxWidth" format="dimension|reference"/>
